<!DOCTYPE html>
<html lang="ja">
<head>
  <% include 'partials/header.ect' %>
  <script src="/socket.io/socket.io.js"></script>
  <script type="text/javascript">
    Vue.config.delimiters = ['[[', ']]'];
  </script>
</head>
<body>
  <div id="chattest" class="container">
    <form @submit.prevent.stop="sendAll()" role="form">
      <div class="input-group">
        <input type="text" class="form-control" v-model="alltext">
        <span class="input-group-btn">
          <button type="submit" class="btn btn-primary">投稿</button>
        </span>
      </div>
    </form>
    <pre>[[alltext]]</pre>
    <ul>
      <li v-for="msg in msgs">[[msg]]</li>
    </ul>
  </div>
  <script src="/js/zinro.js"></script>
</body>
</html>
